<?xml version="1.0"?>
<?xml-model href="http://download.ros.org/schema/package_format3.xsd" schematypens="http://www.w3.org/2001/XMLSchema"?>
<package format="3">
  <name>simple_planner</name>
  <version>1.1.0</version>
  <description>Simple planner for autonomous navigation. It takes waypoints from a CSV file and publishes them as a ROS path message.</description>
  <maintainer email="fam@awadlouis.com">Fam Shihata</maintainer>
  <license>MIT</license>

  <test_depend>ament_copyright</test_depend>
  <test_depend>ament_flake8</test_depend>
  <test_depend>ament_pep257</test_depend>
  <test_depend>python3-pytest</test_depend>

  <export>
    <build_type>ament_python</build_type>
  </export>
</package>
